second problem
#include <iostream>
#include<string.h>
using namespace std;

int main()
{
	int t;
	cin >> t;
	while(t--){int f;
	char str[100], a[4];

	cin >> str;
	f = strlen(str);
	if (strlen(str) > 10)
	{
		f = f - 2;
		cout << str[0] << f << str[strlen(str) - 1];


	}
	else {
		cout << str;
	}
	}
    return 0;
}
third problem
#include <iostream>
#include<string.h>
using namespace std;

int main()
{
	int n, i, j,a[100],f;
	cin >> n;
	cin >> j;
	f = 0;
	for (i = 0;i < n;i++)
	{
		cin >> a[i];
		if(j<a[i])
		f++;
	}
	cout << f;




	return 0;
  
  }
  fourth problem
  #include <iostream>
#include<string.h>
using namespace std;

int main()
{
	int n,r=0,i;
	cin >> n;
	string arr;
	for (i = 0;i < n;i++)
	{
		cin >> arr;
		if (arr.compare("X++")==0 || arr.compare("++X")==0)
		{
			r++;
		}
		if (arr.compare("X--") == 0 || arr.compare("--X") == 0)
			r--;
	}
	cout << r;





	return 0;
}
fifth problem
#include <iostream>
#include<string.h>
using namespace std;

int main()
{
	char a[100];
	int i=0,flag=0;
	cin >> a;
	while
		( a[i] != '\0')
	{
		if (a[i] == a[i + 1] && a[i] == a[i + 2] && a[i] == a[i + 3] && a[i] == a[i + 4] && a[i] == a[i + 4] && a[i] == a[i + 5] && a[i] == a[i + 6] && a[i] == a[i + 7])
		{
			cout << "YES";
			flag = 1;
			break;
		}
		i++;
	}
	if(flag!=1)
	cout << "NO";





	return 0;
}
